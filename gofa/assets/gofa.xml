<mujoco model="abb-gofa">

	
	<compiler meshdir="mesh" inertiafromgeom="true"/>

	<asset>
		<mesh name="link0" file="CRB15000_5kg-950_OmniCore_rev00_LINK0_CAD.STL" />
		<mesh name="link1" file="CRB15000_5kg-950_OmniCore_rev00_LINK1_CAD.STL" />
